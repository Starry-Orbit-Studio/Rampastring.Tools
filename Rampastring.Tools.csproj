<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net40</TargetFrameworks>
    <Version>1.1.1</Version>
    <!-- <OutputPath>bin/$(Configuration)/$(Platform)</OutputPath> -->
    <Authors>Rampastring &amp; frg2089</Authors>
    <Description>Rampastring's Generally Useful Library</Description>
    <Copyright>Copyright Â© Rami '$(Authors)' Pasanen 2016-$([System.DateTime]::Now.Year)</Copyright>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageProjectUrl>https://github.com/Starry-Orbit-Studio/Rampastring.Tools</PackageProjectUrl>
    <RepositoryUrl>$(PackageProjectUrl)</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>

    <PackageVersion>$(Version)-$(Platform)</PackageVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\README.md" Pack="true" PackagePath="" />
  </ItemGroup>

</Project>